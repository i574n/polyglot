## sm'.concat_array_trailing

#!spiral

inl types () =
    rust.types ()
    sm'.types ()

#!spiral

// // test
// // rust=

;[
    "1"
    "2"
    "3"
]
|> fun x =>
    inl code = (a x : _ i32 _) |> sm'.concat_array_trailing "\n"
    code
    |> _assert_eq "1\n2\n3\n"

