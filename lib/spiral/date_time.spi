/// ### ticks
inl ticks (date_time : date_time) : timestamp =
    backend_switch {
        Fsharp = fun () =>
            run_target function
                | Rust (Contract) => fun () => null ()
                | _ => fun () => date_time |> $'_.Ticks'
            : timestamp
        Python = fun () =>
            date_time |> new_time_span (min_value ()) |> total_seconds |> ((*) 10000000) |> convert : timestamp
    }

