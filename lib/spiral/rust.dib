### fix_closure'

#!spiral

inl fix_closure' (depth : u8 * u8) x =
    inl rec loop text (acc : string) n : string =
        if n <= 0
        then acc
        else loop text (acc +. text) (n - 1)
    inl a = depth |> fst |> loop "}" ""
    inl b = depth |> snd |> loop "{" ""
    $'"!x " + !a + "); " + !b + " //"'

#!spiral

//// test

fix_closure' (3, 2) 0i32
|> _assert_eq "0 }}}); {{ //"

#!spiral

//// test

fix_closure' (0, 0) ()
|> _assert_eq "() );  //"

