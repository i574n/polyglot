nominal any' = $"Any"
nominal cow t = $"std_borrow_Cow<`t>"
nominal ref_cell t = $"std_cell_RefCell<`t>"
nominal rc t = $"std_rc_Rc<`t>"
nominal weak_rc t = $"std_rc_Weak<`t>"
