nominal fn_once t = $"FnOnce<`t>"
nominal fn_unit = $"FnUnit"
nominal func0 t = $"Func0<`t>"
nominal func1 t u =
    `(
        typecase t with
        | () => `func0 `u
        | _ => $'' : $"Func0<`t, `u>"
    )
nominal impl t = $"Impl<`t>"
nominal mut' t = $"Mut<`t>"
nominal ref' t = $"Ref<`t>"
nominal send t = $"Send<`t>"
nominal static_ref' t = $"StaticRef<`t>"

