## floor

#!spiral

inl floor forall t {float}. (n : t) : t =
    n |> $'floor'

#!spiral

//// test

0.6 |> floor
|> _assert_eq 0f64

#!markdown

## ceil

#!spiral

inl ceil forall t {float}. (n : t) : t =
    n |> $'ceil'

#!spiral

//// test

0.6 |> ceil
|> _assert_eq 1f64

#!markdown

## round

#!spiral

inl round forall t {float}. (n : t) : t =
    n |> $'round'

#!spiral

//// test

0.5 |> round
|> _assert_eq 0f64

1.5 |> round
|> _assert_eq 2f64

2.5 |> round
|> _assert_eq 2f64

3.5 |> round
|> _assert_eq 4f64

#!markdown

## log_base

#!spiral

inl log_base (new_base : f64) (a : f64) =
    $"System.Math.Log (!a, !new_base)" : f64

#!spiral

//// test

100 |> log_base 10
|> _assert_eq 2

