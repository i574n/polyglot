inl of_array' forall dim t. (items : a dim t) : seq' t =
    backend_switch {
        Fsharp = fun () =>
            fun () =>
                $'for i = 0 to !items.Length - 1 do yield !items.[i]'
            |> new_seq
            |> fun x => x : seq' t
        Python = fun () => $'[item for item in !items]' : seq' t
    }

