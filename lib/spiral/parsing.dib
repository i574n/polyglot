### choice

#!spiral

inl choice parsers : parser _ = fun input =>
    let rec loop = function
        | [] => Error "parsing.choice / no parsers succeeded"
        | p :: ps =>
            match p input with
            | Ok _ as result => result
            | Error _ => loop ps
    loop parsers

