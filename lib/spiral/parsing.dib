### many1

#!spiral

inl many1 p : parser (list _) =
    p >> resultm.map fun first_result, rest =>
        let rec loop acc input =
            match p input with
            | Ok (result, rest) => loop (result :: acc) rest
            | Error _ => acc |> listm.rev, input
        loop [ first_result ] rest

