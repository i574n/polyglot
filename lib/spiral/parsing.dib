### many1_chars

#!spiral

inl many1_chars p : parser string =
    p >> resultm.map fun first_result, rest =>
        let rec loop acc input =
            match p input with
            | Ok (result, rest) => loop (acc +. sm'.obj_to_string result) rest
            | Error _ => acc, input
        loop (first_result |> sm'.obj_to_string) rest

