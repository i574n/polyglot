#!markdown

### try_pick

#!spiral

inl try_pick fn list =
    inl rec body fn = function
        | [] => None
        | x :: xs =>
            match fn x with
            | Some y => Some y
            | None => loop xs
    and inl loop list =
        if var_is list |> not
        then body fn list
        else
            inl fn = join fn
            inl list = dyn list
            join body fn list
    loop list

#!spiral

//// test

listm.init 10i32 id
|> try_pick (fun x => if x = 5i32 then Some x else None)
|> _assert_eq (Some 5i32)

#!markdown

### exists'

#!spiral

inl exists' f x =
    inl length_x : i64 = x |> listm.length
    let rec loop i =
        if i >= length_x
        then false
        elif x |> item i |> f
        then true
        else loop (i + 1)
    loop 0

#!spiral

//// test

[ 'a'; 'b'; 'c' ]
|> exists' fun x => x = 'b'
|> _assert_eq true

[ 'a'; 'b' ]
|> exists' fun x => x = 'c'
|> _assert_eq false

[]
|> exists' fun x => x = 'a'
|> _assert_eq false

