open sm'_operators
open leptos
open rust

inl render (class : () -> string) props : _ svg =
    inl class = class |> rust.emit
    svg ([
        $'"xmlns=\\\"http://www.w3.org/2000/svg\\\""'
        $'"fill=\\\"none\\\""'
        $'"viewBox=\\\"0 0 24 24\\\""'
        $'"stroke-width=\\\"1.5\\\""'
        $'"stroke=\\\"currentColor\\\""'
        $'"class=move || \\\" \\\".to_owned() + &!class(())"'
        ] ++ props) fun () =>
        :>[
            path [
                $'"stroke-linecap=\\\"round\\\""'
                $'"stroke-linejoin=\\\"round\\\""'
                $'"d=\\\"M6 6l12 12m0-12L6 18\\\""'
            ]
        ]
