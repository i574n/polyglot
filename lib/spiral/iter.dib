## enumerate

#!spiral

inl enumerate forall dim {int; number} t. (ar : a dim t) : a dim (unativeint * t) =
    inl (a ar) = ar
    ar
    |> am'.to_vec
    |> into_iter
    |> iter_enumerate
    |> iter_prototypes.iter_collect
    |> am'.vec_map' from_pair
    |> am'.from_vec

