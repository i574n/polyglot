inl binary_search_semi_open_1 arr target left right =
    inl rec body left right =
        if left >= right
        then None
        else
            inl mid = (left + right) / 2
            inl item = index arr mid
            if item = target
            then Some mid
            elif item < target
            then ë£¨í”„ (mid + 1) right
            else ë£¨í”„ left mid
    and inl ë£¨í”„ left right =
        if var_is right |> not
        then body left right
        else
            inl left = dyn left
            join body left right
    ë£¨í”„ left right

inl binary_search_closed_1 arr target left right =
    inl rec body left right =
        if left > right
        then None
        else
            inl mid = (left + right) / 2
            inl item = index arr mid
            if item = target
            then Some mid
            elif item < target
            then ë£¨í”„ (mid + 1) right
            else ë£¨í”„ left (mid - 1)
    and inl ë£¨í”„ left right =
        if var_is right |> not
        then body left right
        else
            inl left = dyn left
            join body left right
    ë£¨í”„ left right

inl binary_search_semi_open_2 arr target left right =
    let rec body left right =
        if left >= right
        then None
        else
            inl mid = (left + right) / 2
            inl item = index arr mid
            if item = target
            then Some mid
            elif item < target
            then ë£¨í”„ (mid + 1) right
            else ë£¨í”„ left mid
    and inl ë£¨í”„ left right = body left right
    ë£¨í”„ left right

inl binary_search_closed_2 arr target left right =
    let rec body left right =
        if left > right
        then None
        else
            inl mid = (left + right) / 2
            inl item = index arr mid
            if item = target
            then Some mid
            elif item < target
            then ë£¨í”„ (mid + 1) right
            else ë£¨í”„ left (mid - 1)
    and inl ë£¨í”„ left right = body left right
    ë£¨í”„ left right

inl get_solutions () =
    [
        "semi_open_1",
